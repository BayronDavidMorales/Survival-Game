# Macro para archivos de recursos basados ​​en YAML de Unity. 
[attr]unityyaml ​​-text merge=unityyamlmerge diferencia

# Macro para todos los archivos binarios que deberían usar Git LFS. 
[attr]lfs -text filter=lfs diff=lfs merge=lfs

# Predeterminado para finales de línea autonormalizados. 
* texto=automático

# Código
 *.cs texto diff=csharp

# Activos de texto de Unity
 *.meta unityyaml 
